  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>conRCM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PaymentID">
            <Value>=Parameters!PaymentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        PR.ID AS PaymentID, Item.Long_Name AS ItemLongName, Item.Short_Name AS ItemShortName, PR.Override_Name, Party.Party_Name, PR.Transaction_Status, 
                         PR.Transaction_Type, PR.Check_Wire_Transfer, Scale.ID AS ScaleID, Scale.Net_Weight AS NetWeight, Settle_dtls.PO_Item_UOM, 
                         Settle_dtls.PO_Item_UOM_NetWeight, Settle_dtls.Rate, Settle.Amount AS AmountDue, Settle.Amount_Paid_Till_Date AS AmountPaidTillDate, 
                         PRD.Apply_Amount AS Amount, PR.Bank_Amount AS HeaderBankAmount, PR.Cash_Amount AS HeaderCashAmount, 
                         PR.Bank_Amount + PR.Cash_Amount AS HeaderTotalAmount, PR.Account_Name_ID, PR.Created_By, PR.Created_Date, PR.Last_Updated_Date, 
                         PR.Transaction_Date
FROM            T_Payment_Receipt AS PR LEFT OUTER JOIN
                         T_Payment_Receipt_Details AS PRD ON PRD.PaymentReceipt_ID = PR.ID LEFT OUTER JOIN
                         T_Settlement AS Settle ON PRD.Settlement_ID = Settle.ID LEFT OUTER JOIN
                         T_Settlement_Details AS Settle_dtls ON Settle_dtls.Settlement_ID_ID = Settle.ID LEFT OUTER JOIN
                         T_Scale AS Scale ON Settle.Scale_ID = Scale.ID LEFT OUTER JOIN
                         T_Scale_Details AS Scale_dtls ON Scale_dtls.Scale_ID = Scale.ID LEFT OUTER JOIN
                         M_Item AS Item ON Scale_dtls.Item_Received_ID = Item.ID LEFT OUTER JOIN
                         M_Party AS Party ON PR.Party_ID = Party.ID LEFT OUTER JOIN
                         M_Bank AS Bank ON Bank.Party_ID = Party.ID
WHERE        (PR.ID = @PaymentID)</CommandText>
      </Query>
