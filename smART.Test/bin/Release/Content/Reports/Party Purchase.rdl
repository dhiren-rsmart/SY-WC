  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>conRCM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="PartyID">
            <Value>=Parameters!PartyName_Param.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Party.ID AS PartyID, Party.Party_Name, Scale.ID AS Ticket, Item.Short_Name, Scale.Net_Weight, Scale.Created_Date
FROM            T_Scale AS Scale LEFT OUTER JOIN
                         M_Party AS Party ON Scale.Party_ID_ID = Party.ID LEFT OUTER JOIN
                         T_Scale_Details AS Scale_Items ON Scale.ID = Scale_Items.Scale_ID LEFT OUTER JOIN
                         M_Item AS Item ON Scale_Items.Item_Received_ID = Item.ID</CommandText>
      </Query>
      <Fields>
        <Field Name="PartyID">
          <DataField>PartyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Party_Name">
          <DataField>Party_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ticket">
          <DataField>Ticket</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Short_Name">
          <DataField>Short_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Net_Weight">
          <DataField>Net_Weight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Created_Date">
          <DataField>Created_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Param_PartyName">
      <Query>
        <DataSourceName>conRCM</DataSourceName>
        <CommandText>SELECT DISTINCT Party_Name, ID
FROM            M_Party
WHERE        (Party_Name IS NOT NULL) AND (Party_Type IN ('Supplier', 'Industrial'))</CommandText>
      </Query>
      <Fields>
        <Field Name="Party_Name">
