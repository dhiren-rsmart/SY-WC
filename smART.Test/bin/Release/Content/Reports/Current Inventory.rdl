    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>conRCM</DataSourceName>
        <CommandText>SELECT        Item.Long_Name, CASE WHEN I.Impact = 'Add' THEN 10 ELSE 1 END AS Impact, SUM(I.Quantity / 2204.62) AS MT, SUM(I.Quantity / 2240) AS GT, 
                         SUM(I.Quantity / 2240 * PO.Price) AS Cost
FROM            T_Inventory AS I INNER JOIN
                         M_Item AS Item ON I.Item_ID_ID = Item.ID INNER JOIN
                         T_Scale AS Scale ON I.Scale_Ticket_ID_ID = Scale.ID LEFT OUTER JOIN
                         T_Purchase_Order_Items AS PO ON Scale.Purchase_Order_ID = PO.PurchaseOrder_ID
GROUP BY I.Impact, Item.Long_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Impact">
          <DataField>Impact</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Long_Name">
          <DataField>Long_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MT">
          <DataField>MT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GT">
          <DataField>GT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
